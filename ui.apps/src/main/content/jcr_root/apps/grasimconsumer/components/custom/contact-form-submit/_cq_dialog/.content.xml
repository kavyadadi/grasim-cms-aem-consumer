<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:sling="http://sling.apache.org/jcr/sling/1.0" xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:cq="http://www.day.com/jcr/cq/1.0" xmlns:jcr="http://www.jcp.org/jcr/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
    jcr:primaryType="nt:unstructured"
    jcr:title="Contact Us Properties"
    sling:resourceType="cq/gui/components/authoring/dialog">
    <content
        granite:class="cmp-container__editor"
        jcr:primaryType="nt:unstructured"
        sling:resourceType="granite/ui/components/coral/foundation/container">
        <items jcr:primaryType="nt:unstructured">
            <column
                jcr:primaryType="nt:unstructured"
                sling:resourceType="granite/ui/components/coral/foundation/container">
                <items jcr:primaryType="nt:unstructured">
                    <callCenterLabel
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldDescription="Add CTA Label"
                        fieldLabel="CTA Label"
                        name="./callCenterLabel"
                        useFixedInlineToolbar="{Boolean}true"/>
                    <callCenterURL
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                        fieldDescription="Add CTA URL"
                        fieldLabel="CTA URL"
                        name="./callCenterURL"
                        rootPath="/content/grasim-com/in/en"/>
                    <goToHomepageLabel
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/textfield"
                        fieldDescription="Add Submit CTA Label"
                        fieldLabel="Button CTA Label"
                        name="./goToHomepageLabel"
                        useFixedInlineToolbar="{Boolean}true"/>
                    <goToHomepageURL
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="granite/ui/components/coral/foundation/form/pathfield"
                        fieldDescription="Add Submit CTA URL"
                        fieldLabel="Button CTA URL"
                        name="./goToHomepageURL"
                        rootPath="/content/grasim-com/in/en"/>
                </items>
            </column>
        </items>
    </content>
</jcr:root>
